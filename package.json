{
  "name": "client-construction-tool",
  "version": "0.1.1-14",
  "private": false,
  "description": "## Project setup ``` npm install ```",
  "author": "wangsiyuan",
  "scripts": {
    "dev":"npm run serve",
    "serve": "vue-cli-service serve --target lib examples/main.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:dev": "vue-cli-service build --target lib --name index src/index.js",
    "prerelease": "npm version prerelease",
    "publish": "npm publish"
  },
  "main": "dist/index.umd.js",
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "css-loader": "^3.5.3",
    "js-base64": "^2.5.2",
    "less": "^3.11.3",
    "less-loader": "^6.1.0",
    "view-design": "^4.2.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-useless-escape": "off",
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
